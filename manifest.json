{
  "manifest_version": 3,
  "name": "Auto Tab Bookmarker",
  "version": "1.0",
  "description": "Automatically bookmark all open tabs every 2 minutes",
  "permissions": [
    "bookmarks",
    "tabs",
    "alarms",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    }
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
